apiVersion: k8s.cni.cncf.io/v1beta1
kind: MultiNetworkPolicy
metadata:
  name:  tcp-port
  annotations:
    k8s.v1.cni.cncf.io/policy-for: tcp-port
spec:
  podSelector: 
    matchLabels:
      name: blue-openshift
  policyTypes:
  - Ingress
  ingress:
  - from:
    ports:
    - protocol: TCP
      port: 8888
